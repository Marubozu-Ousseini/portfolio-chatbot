# infrastructure/terraform.tfvars.example - Example values for portfolio chatbot
aws_region = "us-east-1"
aws_profile = "default"
project_name = "portfolio-chatbot"
environment = "prod"
lambda_memory_size = 512
lambda_timeout = 30
max_tokens = 500
temperature = 0.7

# Optional: S3 prefix for additional RAG docs loaded by Lambda
# s3_data_prefix = "rag-data/"

# Bedrock model ID (must match region availability)
# bedrock_model_id = "meta.llama3-8b-instruct-v1:0"

# WAF rate limit (requests per 5 minutes per IP)
# waf_rate_limit = 100
